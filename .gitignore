# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
# Ignore jetty directory (from hydra-jetty)
/jetty

/config/*.local.yml
/config/*.local.rb
/config/environments/*.local.yml
/config/environments/*.local.rb

# Ignore vim .swp files
.*.swp
.DS_Store
.powrc
*.tmproj

coverage
public/assets

.project
.ffignore
.ruby-version
readme
